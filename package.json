    "babel-core": "^6.24.0",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.0",
    "prettier-check": "^1.0.0"
